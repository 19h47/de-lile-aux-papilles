{
	"require": {
		"timber/timber": "^1.15",
		"vlucas/phpdotenv": "^4.1"
	},
	"name": "19h47/ileauxpapilles",
	"description": "De l'île aux papilles's WordPress theme",
	"type": "wordpress-theme",
	"authors": [
		{
			"name": "Jérémy Levron",
			"email": "jeremylevron@19h47.fr"
		}
	],
	"autoload": {
		"psr-4": {
			"DLAP\\": "./inc"
		},
		"files": [
			"./inc/helpers.php"
		]
	},
	"require-dev": {
		"wp-coding-standards/wpcs": "^2.2",
		"squizlabs/php_codesniffer": "^3.5"
	},
	"config": {
		"allow-plugins": {
			"composer/installers": false
		}
	}
}

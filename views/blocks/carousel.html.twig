<div id="carousel" class="overflow-hidden Carousel Section page-block" data-node-type="CarouselBlock">

	<div class="Site-container">
		<div class="row">
			<div>
				<div class="Carousel__items js-items">
					{% for item in block.items %}
						{% include 'partials/image.html.twig' with { loading: 'eager', image: item, classes: [ 'Carousel__item', 'js-item' ] } %}
					{% endfor %}
				</div>
			</div>
		</div>
	</div>
</div>

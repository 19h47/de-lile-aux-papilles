{#  #}
<div class="Producers Section">
	<div class="Site-container">
		<div class="row">
			<div class="col-12 col-md-10 offset-md-1 overflow-x-auto">
				<table>
					<tbody>
						{% for item in block.items %}
							<tr>
								<td><div class="margin-x-3">{{- item.category ? "<strong>#{item.category}</strong>" | raw -}}</div></td>
								<td><div class="margin-x-3 margin-x-md-0">{{- item.label -}}</div></td>
								<td><div class="margin-x-3 margin-x-md-0">{{- item.title -}}{{- item.title and item.link.url ? '<br>' | raw -}}{{- item.link ? "<a href=\"#{item.link.url}\"#{item.link.target ? " target=\"_blank\""}>#{ item.link.title }</a>" -}}</div></td>
								<td><div class="margin-x-3 margin-left-md-0">{{- item.place ? "#{item.place}" | raw -}}</div></td>
							</tr>
						{% endfor %}
					</tbody>
				</table>
			</div>
		</div>
	</div>
</div>
